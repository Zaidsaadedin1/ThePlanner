{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/Contexts/AddCategoryToggle.js", "src/components/UserDashBoard/SideNav/CategoryModal/CategoryModal.tsx", "src/components/UserDashBoard/SideNav/ListOfCategory/Category/Category.tsx", "src/Contexts/FilterationContext.tsx"],
  "exclude": ["node_modules"]
}
